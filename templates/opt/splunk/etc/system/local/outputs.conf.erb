[tcpout]
defaultGroup = <%= @defaultgroup %>
disabled = <%= @tcpout_disabled %>

<% @groupkeys.sort.each do |group| -%>
[tcpout:<%= group -%>]
server = <%= @target_group[group] -%>:<%= @port %>

<% end -%>

<% @output_title.sort.each do |outputname| -%>
[<%= outputname -%>]
  <% @output_hash[outputname].sort.each do |key,value| -%>
<%= key -%> = <%= value %>
  <% end %>
<% end %>


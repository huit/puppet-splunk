[default]
host = <%= @fqdn %>

<% @input_title.sort.each do |inputname| -%>
[<%= inputname -%>]
  <% @input_hash[inputname].sort.each do |key,value| -%>
  <%= key -%> = <%= value %>
  <% end %>
<% end %>


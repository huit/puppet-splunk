[tcpout]
<% if @tcpout_disabled == 'false' || @tcpout_disabled == 'False' -%>
defaultGroup = <%= @defaultgroup.sort %>
disabled = <%= @tcpout_disabled %>

<% @groupkeys.sort.each do |group| -%>
[tcpout:<%= group -%>]
server = <%= @target_group[group] -%>:<%= @port %>

<% end -%>
<% end -%>
<% if @indexandforward == 'true' || @indexandforward == 'True' -%>
indexAndForward = <%= @indexandforward %>
<% end -%>

<% @output_title.sort.each do |outputname| -%>
[<%= outputname -%>]
<% @output_hash[outputname].sort.each do |key,value| -%>
<%= key -%> = <%= value %>
<% end %>
<% end -%>

